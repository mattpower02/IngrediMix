{
  "$schema": "http://json-schema.org/draft-07/schema#",

  "type": "object",

  "additionalProperties": {
    "type": "object",
    "properties": {
      "age": {
        "type": "integer"
      },
      "gender": {
        "type": "string"
      },
      "image": {
        "type": "string",
        "format": "uri"
      },
      "name": {
        "type": "string"
      }
    },
    "required": ["age", "gender", "image", "name"]
  }
}
